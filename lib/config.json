{
  "py": {
    "commands": [
      {
        "command": "python3",
        "args": ["FILEPATH"]
      }
    ]
  },

  "cpp": {
    "commands": [
      {
        "command": "g++",
        "args": ["-std=c++11", "-o", "bin", "FILEPATH"]
      },
      {
        "command": "./bin",
        "args": []
      }
    ]
  },

  "java": {
    "commands": [
      {
        "command": "javac",
        "args": ["FILEPATH"]
      },
      {
        "command": "java",
        "args": ["FILENAME"]
      }
    ]
  },

  "sh": {
    "commands": [
      {
        "command": "FILEPATH"
      }
    ]
  },

  "c": {
    "commands": [
      {
        "command": "cc",
        "args": ["-std=c99", "-o", "bin", "FILEPATH"]
      },
      {
        "command": "./bin"
      }
    ]
  }
}
